storm: $(OBJECTS) $(ALL_OBJECTS)
	@echo "Linking..."
	@$(CC) -o $(@) $(OBJECTS) $(ALL_OBJECTS) $(LIBS) $(LDFLAGS)

install: all

package-source:
	mkdir -p /root/Development/chaos/storm.old/package-source/generic
	-cp -f makefile.template $(EXTRA_FILES) /root/Development/chaos/storm.old/package-source/generic
	for source in $(SOURCES) ; do cp $$source /root/Development/chaos/storm.old/package-source/generic ; done
package-check: package-source
	     cd package-source && ./configure $(ALL_ARGUMENTS) && $(MAKE) && $(MAKE) clean
	     find package-source -name makefile -exec rm {} ';'
	     rm package-source/config.h
package: package-check
	rm -rf storm-0.5.2a
	mv package-source storm-0.5.2a
	tar cvIf storm-0.5.2a.tar.bz2 storm-0.5.2a
	     
