<autochaos>
  <package name="ipv4" version="0.0.1" type="server" license="GPL">
    <library type="depend">ipc</library>
    <library type="depend">log</library>
    <library type="depend">string</library>
    <library type="depend">system</library>
    <library type="depend">memory</library>
    <library type="depend">network</library> 
    <library type="depend">mutex</library>
    <library type="depend">random</library>
    <library type="depend">list</library>
    <library type="depend">time</library>

    <makefile>makefile</makefile>

    <!-- Sources. -->    

    <source>arp.c</source>
    <source>dhcp.c</source>
    <source>forward.c</source>
    <source>icmp.c</source>
    <source>ipv4.c</source>
    <source>route.c</source>
    <source>socket.c</source>
    <source>tcp.c</source>
    <source>udp.c</source>
  </package>
</autochaos>

