gcc -c -I../../Include -g -D__STORM_KERNEL_MODULE__ interface_module.c

ar cru bash.a shell.o y.tab.o general.o make_cmd.o print_cmd.o \
dispose_cmd.o execute_cmd.o variables.o copy_cmd.o error.o \
expr.o flags.o jobs.o subst.o hash.o mailcheck.o test.o \
trap.o alias.o   braces.o 	   unwind_prot.o  input.o bashhist.o \
version.o bashline.o bracecomp.o builtins/libbuiltins.a posix.a

ld -o bash.so -shared interface_module.o bash.a libbuiltins.a libglob.a libreadline.a libtilde.a libtermcap.a posix.a
gzip bash.so

    mount -o loop /mnt/chaos.flp /mnt/floppy
    cp bash.so.gz /mnt/floppy/system/modules/bash.so.gz
    umount /mnt/chaos.flp
