mkdir  ModuleRelease

make deps \
    preprocessor_flags="-D__STORM_MODULE__" \
    INCLUDES="-I../../Include" \
    --directory=ModuleRelease \
    --makefile=../Makefile 

make \
    compile_flags="-O3 -funroll-loops -fomit-frame-pointer" \
    preprocessor_flags="-D__STORM_MODULE__" \
    LD_SCRIPT="../kernel.ld" \
    INCLUDES="-I../../Include" \
    LIBRARIES="../Libraries/string.a" \
    --directory=ModuleRelease \
    --makefile=../Makefile 
